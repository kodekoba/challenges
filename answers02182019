    function SettingAndSwapping(){
        var myNumber = 42;
        var myName = 'Jeff';
        var temp = myName;
        myName = myNumber;
        myNumber = temp;
        console.log('My number is ' + myName);
        console.log('My name is '+ myNumber);
    }
    function PrintAndCount(){
        for (var i = 0; 5 + 5 * i <= 4096; i++){
            document.write(5 + 5 * i + " ");
        }
        console.log("Count is "+ i); 
    }
    function PrintNeg52to1066(){
        for (var i = -52; i <= 1066; i++){
            document.write(i + " ");
        }
    }
    function MultiplesOfSix(){
        var num = 6;
        while (num <= 60000){
            document.write(num + " ");
            num = num + 6;
        }
    }
    function beCheerful(){
        console.log("good morning!")
    }
    function DontWorryBeHappy(){
        for (i = 0; i < 98; i++){
            beCheerful();
        }
    }
    function CountingTheDojoWay(){
        for (i = 1; i <= 100; i++){
            if (i % 5 == 0){
                document.write("Coding ");
            }
            if (i % 10 == 0){
                document.write("Dojo ");
            }
            else if (i % 5 > 0){
                document.write(i + " ");
            }
        }
    }
    function MultiplesOf3ButNotAll(){
        for (i = -300; i <= 0; i = i + 3){
            if (i == -3 || i == -6){
            }
            else{
                document.write(i + " ");
            }
        }
    }
    function WhatDoYouKnow(incoming){
        console.log(incoming);
    }
    function PrintingIntegersWithWhile(){
        var num = 2000
        while (num <= 5280){
            document.write(num + " ");
            num++;
        }
    }
    /*function WhoaThatSuckersHuge(){
