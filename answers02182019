    function Countdown(num){
        var arr = [];
        for (i = num; i >= 0; i = i - 1){
            arr.push(i);
        }
        console.log("Array length is " + arr.length);
        return arr;
    }
    function PrintAndReturn(arr){
        /*var arr = [];
        arr.push(val1);
        arr.push(val2);*/
        document.write(arr[0]);
        return arr[1];
    }
    function FirstPlusLength(arr){
        if (isNaN(arr[0])){
            return("Hey you can't add those silly goose!");
        }
        else{
            sum = arr[0] + arr.length;
            return sum;
        }
    }
    function ValuesGreaterThanSecond(){
        var arr = [1,3,5,7,9,13];
        count = 0;
        for (i = 0; i < arr.length; i++){
            if (arr[i] > arr[1]){
                document.write(arr[i] + " ");
                count++;
            }
        }
        return "The number of values greater than the second value is " + count;
    }
    function ValuesGreaterThanSecondGen(arr){
        var arrnew = [];
        count = 0;
        for (i = 0; i < arr.length; i++){
            if (arr[i] > arr[1]){
                arrnew.push(arr[i]);
                count++;
            }
        }
        if (arr.length == 1){
                document.write("There's no second value to compare with :( <br>");
            }
        else document.write("The number of values greater than the second value is " + count);
        return arrnew;
    }
    function ThisLengthThatValue(num1,num2){
        var arr = [];
        for (i = 0; i < num1; i++){
            arr.push(num2);
        }
        if (num1 == num2){
            document.write("Jinx!");
        }
        return arr;
    }
    function FitTheFirstValue(arr){
        if (arr[0] > arr.length){
            document.write("Too big! ")
        }
        else if (arr[0] < arr.length){
            document.write("Too small! ")
        }
        else if (arr[0] == arr.length){
            document.write("Just right! ")
        }
    }
    function FahrenheitToCelcius(fDegrees){
        var Celcius = (5 / 9) * (fDegrees - 32);
        return Celcius + " deg C";
    }
    function celsiusToFahrenheit(cDegrees){
        var Fahrenheit = (9/5 * cDegrees) + 32;
        return Fahrenheit + " deg F"
    }
    function BiggieSize(arr){
        for (i = 0; i < arr.length; i++){
            if (arr[i] > 0){
                arr[i] = "big";
            }
        }
        console.log(arr);
    }
    function PrintLowReturnHigh(arr){
        var min = Math.min(...arr);
        var max = Math.max(...arr);
        document.write(min);
        return max;
    }
    function PrintOneReturnAnother(arr){
        document.write(arr[arr.length - 2]);
        for (i = 0; i < arr.length; i++){
            if (arr[i] % 2 == 1){
                    return arr[i];
                    break;
                }
        }
    }
    function double(arr){
        arrnew = [];
        for (i = 0; i < arr.length; i++){
            arrnew.push(arr[i] + arr[i]);
        }
        return arrnew;
    }
    function CountPositives(arr){
        count = 0;
        for (i = 0; i < arr.length; i++){
            if (arr[i] > 0){
                count++
            }
        }
        arr[arr.length - 1] = count;
        return arr;
    }
    /*function EvensAndOdds(arr){ //idk

    }*/
    function IncrementTheSeconds(arr){
        for (i = 0; i < arr.length; i++){
            if (arr[i] % 2 == 1){
                arr[i] = arr[i] + 1;
            }
            document.write(arr[i] + " ");
        }
        return arr;
    }
    /*function PreviousLengths(arr){ //crashing
        for (i = 0; i < arr.length; i++){
