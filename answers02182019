    function WhoaThatSuckersHuge(){
        var sum = 0;
        for (var i = 1; i <= 300000; i++){
            if (i % 2 == 1){
                sum = sum + i;
                sum = sum - i;
            }
        }
        return sum;
    }
    function YouSayItsYourBday(num1,num2){
        if ((num1 == 4 && num2 == 22) || (num1 == 22 && num2 == 4)){
            console.log("How did you know??!!!!*#&$(@&*$");
        }
        else {
            console.log("Just another day....");
        }
    }
    function CountdownByFours(){
        var num = 2016
        while (num > 0){
            console.log(num);
            num = num - 4;
        }
    }
    function LeapYear(year){
        if (year % 400 == 0){
            console.log("Yes");
        }
        else if (year % 100 == 0){
            console.log("No");
        }
        else if (year % 4 == 0){
            console.log("Yes");
        }
        else {
            console.log("No");
        }
    }
    /*function FlexibleCountdown(lowNum, highNum, mult){ //WHAT ARE THESE ASKING??
        
